
public class Floor extends GuestSpace {
	
	double height;
	
	public Floor() {
		height = 12;
	}
	
	public Floor(int height) {
		this.height = height;
	}
	
	public double getHeight() {
		return height;
	}
	
	public void callElevator(Elevator.DIRECTION direction) {
		Building.getOperator().callElevator(this, direction);
	}

}
